{
  "name": "jsonl-viewer",
  "displayName": "JSONL Pretty Viewer",
  "description": "Opens .jsonl files with pretty-printing and syntax highlighting. Toggle between formatted and raw views with a single click.",
  "version": "0.1.0",
  "publisher": "tamirdub",
  "license": "MIT",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.85.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tamirdub/jsonl-viewer"
  },
  "bugs": {
    "url": "https://github.com/tamirdub/jsonl-viewer/issues"
  },
  "homepage": "https://github.com/tamirdub/jsonl-viewer#readme",
  "categories": [
    "Other",
    "Visualization"
  ],
  "keywords": [
    "jsonl",
    "json lines",
    "json",
    "viewer",
    "prettify",
    "syntax-highlighting",
    "ndjson",
    "data"
  ],
  "activationEvents": [
    "onCustomEditor:jsonlViewer.prettyView",
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "jsonlViewer.prettyView",
        "displayName": "JSONL Pretty Viewer",
        "selector": [
          {
            "filenamePattern": "*.jsonl"
          }
        ],
        "priority": "default"
      }
    ],
    "commands": [
      {
        "command": "jsonlViewer.openPretty",
        "title": "JSONL: Open File Pretty"
      }
    ]
  }
}
